#include<stdio.h>

struct student{
    char roll_number[10];
    int marks;
    char name[30];
};
int main(){
    int i;
    struct student s1[5];
    
    for(i=0;i<5;i++){
        printf("Enter your name: ");
        scanf("%s",&s1[i].name);
    
        printf("Enter your roll_number: ");
        scanf("%s",&s1[i].roll_number);
        
        printf("Enter your marks: ");
        scanf("%d",&s1[i].marks);
    }
    
    return 0;
}

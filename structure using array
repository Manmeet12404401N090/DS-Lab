#include <stdio.h>
    struct student {
        char name[30];
        char roll_number[10];
        int marks;
    };
    int main() {
        struct student s1[5];
        for(int i = 0; i<5; i++) {
            printf("Enter the name: ");
            scanf("%s",s1[i].name);
            printf("Enter the value of roll_no: ");
            scanf("%s",s1[i].roll_number);
            printf("Enter the value of marks: ");
            scanf("%d",&s1[i].marks);
        }
        return 0;
    }

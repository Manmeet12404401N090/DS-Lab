#include <stdio.h>

int main() {
    int arr[5], n, i;

    printf("Enter the number of elements: ");
    scanf("%d", &n);

    printf("Enter elements: ", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    printf("The array elements are: ");
    for (i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }
    n--;
    printf("Updated array: ");
    for(i = 0; i < n; i++){
    printf("%d", arr[i]);
}

    return 0;
}
   
